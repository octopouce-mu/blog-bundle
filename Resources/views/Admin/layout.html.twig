{% extends '@OctopouceAdmin/layout.html.twig' %}

{% trans_default_domain 'blog' %}

{% if form is defined %}
    {% form_theme form '@OctopouceAdmin/Form/layout.html.twig' %}
{% endif %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('bundles/octopouceblog/css/blog.css') }}">
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('bundles/octopouceblog/js/blog.js') }}"></script>
{% endblock %}